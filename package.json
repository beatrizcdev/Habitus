{
  "name": "growup-squad29",
  "version": "1.0.0",
  "description": "Aplicação web de lista de tarefas gamificada com funcionalidades de gestão financeira.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --runInBand --config backend/jest.config.ts",
    "dev": "nodemon --exec tsx ./src/server.ts",
    "knex": "node --import tsx ./node_modules/knex/bin/cli.js",
    "knex:seed": "tsx node_modules/knex/bin/cli.js seed:run --knexfile knexfile.ts",
    "knex:migrate": "tsx node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts",
    "json-server": "npx json-server --watch db.json --port 3001"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "knex": "^3.1.0",
    "node-cron": "^4.0.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  }
}
